    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint . --ext .ts,.tsx",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "react",
    "components",
    "ui",
    "design-system",
    "shadcn"
  ],
  "author": "DATCO",
  "license": "MIT",
